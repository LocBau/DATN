{
  "expo": {
    "name": "TimeTable_App",
    "slug": "TimeTable_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist":{
        "NSLocation*UsageDescription":"Allow $(PRODUCT_NAME) to use your location",
        "NSLocationUsageDescription":"Allow $(PRODUCT_NAME) to use your location",
        "NSLocationWhenInUseUsageDescription":"Allow $(PRODUCT_NAME) to use your location",
        "NSLocationAlwaysUsageDescription":"Allow $(PRODUCT_NAME) to use your location",
        "NSLocationAlwaysAndWhenInUseUsageDescription":"Allow $(PRODUCT_NAME) to use your location",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },  
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
          "ACCESS_BACKGROUND_LOCATION"
        ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "81a0efe5-52e3-45d0-8d3f-4234785fcd8c"
      }
    }
  }
}
